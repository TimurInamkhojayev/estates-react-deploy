Users/timurinamkhojayev/Desktop/inamkhojayev-java-spring-project-main/estate-client-react/src/components/admin/EstateTable.js"],"sourcesContent":["import React, {Fragment} from 'react'\nimport {Button, Form, Grid, Image, Input, Table} from 'semantic-ui-react'\nimport EstateForm from './EstateForm'\n\nfunction EstateTable({\n                         estates,\n                         estateId,\n                         estateTitle,\n                         estatePoster,\n                         estateDescription,\n                         estateContact,\n                         estatePrice,\n                         estateAddress,\n                         estateTextSearch,\n                         handleInputChange,\n                         handleAddEstate,\n                         handleDeleteEstate,\n                         handleSearchEstate\n                     }) {\n    let estateList\n    if (estates.length === 0) {\n        estateList = (\n            <Table.Row key='no-estate'>\n                <Table.Cell collapsing textAlign='center' colSpan='5'>No estate</Table.Cell>\n            </Table.Row>\n        )\n    } else {\n        estateList = estates.map(estate => {\n            return (\n                <Table.Row key={estate.id}>\n                    <Table.Cell collapsing>\n                        <Button\n                            circular\n                            color='red'\n                            size='small'\n                            icon='trash'\n                            onClick={() => handleDeleteEstate(estate.id)}\n                        />\n                    </Table.Cell>\n                    <Table.Cell>\n                        {estate.poster ?\n                            <Image src={estate.poster} size='tiny' bordered rounded/> :\n                            <Image src='/images/estate-poster.jpg' size='tiny' bordered rounded/>\n                        }\n                    </Table.Cell>\n                    <Table.Cell>{estate.id}</Table.Cell>\n                    <Table.Cell>{estate.title}</Table.Cell>\n                    <Table.Cell>{estate.description}</Table.Cell>\n                    <Table.Cell>{estate.contact}</Table.Cell>\n                    <Table.Cell>{estate.price}</Table.Cell>\n                    <Table.Cell>{estate.address}</Table.Cell>\n                    <Table.Cell>{estate.createdAt}</Table.Cell>\n                </Table.Row>\n            )\n        })\n    }\n\n    return (\n        <Fragment>\n            <Grid stackable divided>\n                <Grid.Row columns='2'>\n                    <Grid.Column width='5'>\n                        <Form onSubmit={handleSearchEstate}>\n                            <Input\n                                action={{icon: 'search'}}\n                                name='estateTextSearch'\n                                placeholder='Search by Id or Title'\n                                value={estateTextSearch}\n                                onChange={handleInputChange}\n                            />\n                        </Form>\n                    </Grid.Column>\n                    <Grid.Column>\n                        <EstateForm\n                            estateId={estateId}\n                            estateTitle={estateTitle}\n                            estatePoster={estatePoster}\n                            estateContact={estateContact}\n                            estatePrice={estatePrice}\n                            estateAddress={estateAddress}\n                            estateDescription={estateDescription}\n                            handleInputChange={handleInputChange}\n                            handleAddEstate={handleAddEstate}\n                        />\n                    </Grid.Column>\n                </Grid.Row>\n            </Grid>\n            <Table compact striped selectable>\n                <Table.Header>\n                    <Table.Row>\n                        <Table.HeaderCell width={1}/>\n                        <Table.HeaderCell width={3}>Poster</Table.HeaderCell>\n                        <Table.HeaderCell width={2}>ID</Table.HeaderCell>\n                        <Table.HeaderCell width={3}>Title</Table.HeaderCell>\n                        <Table.HeaderCell width={4}>Description</Table.HeaderCell>\n                        <Table.HeaderCell width={2}>Contact</Table.HeaderCell>\n                        <Table.HeaderCell width={1}>Price</Table.HeaderCell>\n                        <Table.HeaderCell width={2}>Address</Table.HeaderCell>\n                        <Table.HeaderCell width={2}>CreatedAt</Table.HeaderCell>\n                    </Table.Row>\n                </Table.Header>\n                <Table.Body>\n                    {estateList}\n                </Table.Body>\n            </Table>\n        </Fragment>\n    )\n}\n\nexport default EstateTable"],"mappings":"AAAA,MAAO,CAAAA,KAAK,EAAGC,QAAQ,KAAO,OAAO,CACrC,OAAQC,MAAM,CAAEC,IAAI,CAAEC,IAAI,CAAEC,KAAK,CAAEC,KAAK,CAAEC,KAAK,KAAO,mBAAmB,CACzE,MAAO,CAAAC,UAAU,KAAM,cAAc,QAAAC,GAAA,IAAAC,IAAA,gCAAAC,IAAA,IAAAC,KAAA,yBAErC,QAAS,CAAAC,WAAWA,CAAAC,IAAA,CAcI,IAdH,CACIC,OAAO,CACPC,QAAQ,CACRC,WAAW,CACXC,YAAY,CACZC,iBAAiB,CACjBC,aAAa,CACbC,WAAW,CACXC,aAAa,CACbC,gBAAgB,CAChBC,iBAAiB,CACjBC,eAAe,CACfC,kBAAkB,CAClBC,kBACJ,CAAC,CAAAb,IAAA,CAClB,GAAI,CAAAc,UAAU,CACd,GAAIb,OAAO,CAACc,MAAM,GAAK,CAAC,CAAE,CACtBD,UAAU,cACNlB,IAAA,CAACH,KAAK,CAACuB,GAAG,EAAAC,QAAA,cACNrB,IAAA,CAACH,KAAK,CAACyB,IAAI,EAACC,UAAU,MAACC,SAAS,CAAC,QAAQ,CAACC,OAAO,CAAC,GAAG,CAAAJ,QAAA,CAAC,WAAS,CAAY,CAAC,EADjE,WAEJ,CACd,CACL,CAAC,IAAM,CACHH,UAAU,CAAGb,OAAO,CAACqB,GAAG,CAACC,MAAM,EAAI,CAC/B,mBACIzB,KAAA,CAACL,KAAK,CAACuB,GAAG,EAAAC,QAAA,eACNrB,IAAA,CAACH,KAAK,CAACyB,IAAI,EAACC,UAAU,MAAAF,QAAA,cAClBrB,IAAA,CAACR,MAAM,EACHoC,QAAQ,MACRC,KAAK,CAAC,KAAK,CACXC,IAAI,CAAC,OAAO,CACZC,IAAI,CAAC,OAAO,CACZC,OAAO,CAAEA,CAAA,GAAMhB,kBAAkB,CAACW,MAAM,CAACM,EAAE,CAAE,CAChD,CAAC,CACM,CAAC,cACbjC,IAAA,CAACH,KAAK,CAACyB,IAAI,EAAAD,QAAA,CA